{
	"schemaVersion": "0.1.0",
	"name": "YouTube",
	"behavior": "create",
	"noteContentFormat": "\n# {{schema:name}}\n\n![{{schema:name}}]({{url}})",
	"properties": [
		{
			"name": "aliases",
			"value": "[\\\"{{schema:name|replace:\\\"\\\\\"\\\":\\\"\\\\\\\\\"\\\"}}\\\"]",
			"type": "multitext"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{schema:author|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "title",
			"value": "{{schema:name}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{schema:uploadDate|date:YYYY-MM-DD}}",
			"type": "date"
		},
		{
			"name": "image",
			"value": "{{schema:thumbnailUrl|slice:0}}",
			"type": "text"
		},
		{
			"name": "createdAt",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://www.youtube.com/watch?v="
	],
	"noteNameFormat": "{{schema:name|safe_name}}",
	"path": "Inbox"
}